alert_msg=""
alert_life=0
msg_temp=false

function alert(msg, temp)
	alert_msg=msg
	alert_life=80
	msg_temp=temp
end

function draw_bar(x, y, dmg)
	spr(dmg_icons[dmg], x, y)
	local bgcol = 5
	local col = dmg_cols[dmg]
	local fill = p.stats[dmg]
	rectfill(x+8, y+2, x+8+32, y+6, bgcol)
	rectfill(x+8, y+2, x+8+fill, y+6, col)
end

function draw_health(x, y)
	local fill=p.health/p.maxhealth
	local bgcol = 5
	local col = 8
	rectfill(x, y, x+64, y+6, bgcol)
	rectfill(x, y, x+(fill*64), y+6, col)
	
	local s = flr(p.health).."/"..flr(p.maxhealth)
	print(s, x+2, y+1, 7)
end

function draw_item(x, y)
	local fill = max(p.cooldown,0)/p.gun().cool

	rectfill(x, y, x+10, y+10, 5)
	
	rect(x, y, x+fill*10, y+10, 6)
	
	rect(x, y, x+10, y+10, 7)
	spr(p.gun().sp, x+1, y+1)
	
	spr(dmg_icons[p.gun().dmg_t], x+2, y-9)
end

function draw_ui()
	alert_life = max(0, alert_life-1)

	camera(0, 0)
	ctd_str = tostr(ctd)
	ctd_str = sub(ctd_str, 1, 3)
	print(ctd_str, 60, 8, 7)
	
	if alert_life > 0 then
		local x = 64-#alert_msg*2
		rectfill(x-1, 60, x+#alert_msg*4, 76, 0)
		print(alert_msg, x,61, 7)
	
		if msg_temp then
			print("temporary", 64-9*2,70, 7)
		end
	end
	
	
	draw_bar(1, -1, "em")
	draw_bar(43, -1, "therm")
	draw_bar(85, -1, "phys")
	
	print(p.gun().name, 15, 114, 7)
	draw_health(14, 120)
	draw_item(1, 116)
	
	print("round "..round, 90, 121, 7)
	
	debug.draw(7)
	--debug.clear()
end

function onscreen(e, pad)
	local hb = {
		x=camx-pad,
		y=camy-pad,
		w=128+pad,
		h=128+pad
	}
	
	return col(e, hb)
end
