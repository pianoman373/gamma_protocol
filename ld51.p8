function p.give_gun()
	if #guns > 0 then
		sfx(13)
		local g = rnd(guns)
		del(guns, g)
		
		add(p.guns, g)
		p.held_gun=#p.guns
		
		p.msg=g.name
		p.msg_life=60
	else
		p.maxhealth+=10
		p.msg="+10 health"
		p.msg_life=60
	end
end

function p.gun()
	return p.guns[p.held_gun]
end

function p.dmg(amt)
	if (p.flash>0) return

	shake(3, 3)
	sfx(14)
	p.flash=40
	p.health-=amt*diff.dmg_multiplier
	
	if p.health < 1 then
		dead=true
		spawn_ef(p.x+4, p.y+8, 6)
		sfx(17)
	end
end

function p.cycle()
	p.cooldown=0
	sfx(13)
	p.held_gun+=1
	if (p.held_gun > #p.guns) p.held_gun=1
end

function p.shoot()
	local g = p.gun()

	if p.cooldown > 0 then
		return
	end
	p.cooldown = g.cool

