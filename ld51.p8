	if (p.flash>0) return

	shake(3, 3)
	sfx(14)
	p.flash=40
	p.health-=amt*diff.dmg_multiplier
	
	if p.health < 1 then
		dead=true
		spawn_ef(p.x+4, p.y+8, 6)
		sfx(17)
	end
end

