function enemy(t)
	local e = {
		t=t,
		x=0,
		y=0,
		w=8,
		h=8,
		dx=0,
		dy=0,
		flash=0,
		health=t.health,
		sp={32,33},
		anim=1,
		flip=false
	}
	
	function e.dmg(self, bul)
		self.dx = bul.dx*bul.r*0.5
		self.dy = bul.dy*bul.r*0.5
		self.flash = 40
		self.health-=bul.dmg
		
		if self.health<1 then
			self:die()
		end
	end
	
	function e.die(self)
		del_entity(2, self)
	end
	
	function e.shoot(self)
		local dx, dy = nrm(p.x-self.x, p.y-self.y)
	
		bullet(
			self.x+4,
			self.y+4,
			dx+rnd(0.5)-0.25,
			dy+rnd(0.5)-0.25,
			e.t.bul_size,
			"enemy",
			e.t.dmg
		)
	end
	
	function e.update(self)
		if not onscreen(self, 64) then
			del_entity(2, self)
			return
		end
	
		self.anim+=0.05
		if (self.anim > #self.sp+1) self.anim-=#self.sp
	
		self.dx *= 0.8
		self.dy *= 0.8
		
		if (self.dx > 0.1) self.flip=true
		if (self.dx < -0.1) self.flip=false	
		
		if e.t.shoots then
			if rnd(e.t.shoot_chance) < 1 then
				self:shoot()
			end
		end
		
		if rnd(e.t.move_chance) < 1 then
			self.dx, self.dy = nrm(p.x-self.x, p.y-self.y)
			self.dx *= e.t.move_speed
			self.dy *= e.t.move_speed
		end
		
		move(self)
		
		if (self.flash > 0) self.flash-=1
	
		if col(self, p) then
			p.dmg(self.t.dmg)
		end
	end
	
	function e.draw(self)
		if self.flash > 0 then
			flash(7)
		end
		local s = self.t.sp[flr(self.anim)]
		spr(s, self.x, self.y, 1, 1, self.flip)
		unflash()
		
	end
	
	return e
end

function bullet(x, y, dx, dy, r, dmg_t, dmg)
	sfx(9)
	local e = {
		x=x,
		y=y,
		w=r*2,
		h=r*2,
		dx=dx,
		dy=dy,
		life=10,
		col=dmg_cols[dmg_t],
		r=r,
		dmg=dmg,
		dmg_t=dmg_t
	}
	e.x -= e.w/2
	e.y -= e.h/2
	
	function e.update(self)
		if not onscreen(self, 0) then
			del_entity(1, self)
			return
		end
		
		local cx, cy = move(self)
		
		if cx or cy then
			self:explode()
			return
		end
		
		self.life-=0.1
		if self.life < 0 then
			self:explode()
		end
		
		if self.dmg_t=="enemy" then
			if col(self, p) then
				p.dmg(self.dmg)
				self:explode()
				return
			end
			
		else
			local cols = entity_col(2, self)
			
			for i in all(cols) do
				i:dmg(self)
				self:explode()
				return
			end
		end
	end
	
	function e.explode(self)
		if (self.dmg_t != "enemy") sfx(11)
		explosion(self.x, self.y, self.col, self.r*2)
		del_entity(1, self)
	end
	
	function e.draw(self)
		local x = self.x+self.r
		local y = self.y+self.r
	
		circfill(x, y, self.r+1, self.col)
		circfill(x, y, self.r, 7)
	end
	
	spawn_entity(1, e)
end
