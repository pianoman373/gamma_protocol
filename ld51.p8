	end
	if btn(5) then
		p.shoot()
	end
	
	move(p)
	
	local moving = abs(p.dx) > 0.1 or abs(p.dy) > 0.1
	
	if moving then
		p.anim -= 1
		
		if p.anim < 0 then
			p.anim += 20
		end
	else
		p.anim = 0
	end
	
	local e = find_nearest(p.x+4, p.y+4, 2, 12)
	
