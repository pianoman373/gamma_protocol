--[entity system]--

entities = {}

function entity_count(layer)
	if entities[layer] then
		return #entities[layer]
	end
	return 0
end

function spawn_entity(layer, e)
	if (not entities[layer]) entities[layer] = {}
	add(entities[layer], e)
end

function del_entity(layer, e)
	del(entities[layer], e)
end

function entity_col(layer, hb)
	local r = {}
	for e in all(entities[layer]) do
		if col(hb, e) then
			add(r, e)
		end
	end
	return r
end

function update_entities(layer)
	local l = entities[layer]
	if l then
		for e in all(l) do
			e:update()
		end
	end
end

function draw_entities(layer)
	local l = entities[layer]
	if l then
		for e in all(l) do
			e:draw()
		end
	end
end

function find_nearest(x, y, layer, range)
	local ldist = -1
	local le = nil
	local range = range or 10000000
	for i in all(entities[layer]) do
		
		local dx = abs(x-(i.x+i.w/2))/8
		local dy = abs(y-(i.y+i.h/2))/8
		local dist = sqrt(dx*dx) + sqrt(dy*dy)
		
		if le then
			if dist < ldist then
				le = i
				ldist = dist
			end
		else
			le = i
			ldist = dist
		end
	end
	
	
	if ldist < range then
		return le
	end
end
