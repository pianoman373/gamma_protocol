function p.give_gun()
	if #guns > 0 then
		sfx(13)
		local g = rnd(guns)
		del(guns, g)
		
		add(p.guns, g)
		p.held_gun=#p.guns
		
		p.msg=g.name
		p.msg_life=60
	else
		p.maxhealth+=10
		p.msg="+10 health"
		p.msg_life=60
	end
