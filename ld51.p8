ctd = 10
lasttime = 0
camx=63*8-64
camy=33*8
shakex=0
shakey=0
title=true
dead=false
round=0

diff={
	enemy_cap=2,
	dmg_multiplier=1,
	spawn_chance=100
}

function shake(x, y)
	shakex+=x
	shakey+=y
end

function tick()
	round+=1
	sfx(10)
	lvup(p.x+4, p.y+4)
	
	diff.enemy_cap+=0.5
	diff.dmg_multiplier+=0.1
	diff.spawn_chance*=0.8
	
	if (modifier and modifier.undo) modifier.undo()
	
	modifier = rnd(modifiers)
	if rnd(3) < 1 then
		modifier=modifiers[1]
	end
	
	alert(modifier.msg, modifier.temp)
	modifier.action()
end

function spawn_enemy(t)
	if entity_count(2) >= flr(diff.enemy_cap) then
		return
	end

	for i=1,30 do
		local x = flr((camx+rnd(128))/8)
		local y = flr((camy+rnd(128))/8)
		
		--dont spawn too close to player
		if dist(p.x/8, p.y/8, x, y) < 4 then
			goto continue
		end
	
		--dont spawn in walls
		local tile = mget(x, y)
		if fget(tile, 0) then
			goto continue
		end
	
		local e = enemy(t)
		e.x = x*8
		e.y = y*8
		spawn_entity(2, e)
		
		spawn_ef(e.x+4, e.y+8, 9)
		sfx(15)
		break
		
		::continue::
	end
end

function _init()
	if title then
		init_title()
	else
		start()
	end
end

function start()
	music(0)
	pal({[0]=0,1,2,3,132,5,6,7,8,4,10,142,12,141,133,143},1)
	p.give_gun()
	
	for i=0,#guns do
		--p.give_gun()
	end
end

function _update60()
	if (title or dead) return
 
	if rnd(diff.spawn_chance) < 1 then
		local e = rnd(enemies)
	
		spawn_enemy(e)
		
	end

	ctd -= time()-lasttime
	lasttime = time()
	
	if ctd < 0 then
		ctd += 10
		tick()
	end
	
	p.update()
	
	--bullets
	update_entities(1)
	
	--enemies
	update_entities(2)
	
	update_particles()
end

function update_cam()
	local pad = 30
	
	if (camx+128-pad < p.x) camx+=1
	if (camx+pad > p.x) camx-=1

	if (camy+128-pad < p.y) camy+=1
	if (camy+pad > p.y) camy-=1

	camx = mid(0, camx, 896)
	camy = mid(0, camy, 384)
end

function _draw()
	if title then
		draw_title()
		return
	end

	update_cam()
	cls()
	
	
	
	local dx=rnd(2*shakex)-shakex
	local dy=rnd(2*shakey)-shakey
	shakex=max(0,shakex-0.25)
	shakey=max(0,shakey-0.25)
	camera(camx+dx, camy+dy)
	map(0, 0, 0, 0, 128, 128)
	
	--bullets
	draw_entities(1)
	
	--enemies
	draw_entities(2)
	
	draw_particles()
	
	if (not dead) p.draw()
	
	draw_ui()
	
	if dead then
		print("you died!", 51, 53, 0)
		print("you died!", 50, 52, 7)
		
		print("press 🅾️ to reset", 36, 63, 0)
		print("press 🅾️ to reset", 35, 62, 7)
		
		print("survived "..round.." rounds", 34, 83, 0)
		print("survived "..round.." rounds", 33, 82, 7)
		
		if btnp(🅾️) then
			run()
		end
	end
end
