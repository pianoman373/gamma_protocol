end

function p.update()
	if (p.msg_life>0) p.msg_life-=1
	
	if map_col(p, 1) then
		p.give_gun()
		
		local tx = flr(p.x/8)
		local ty = flr(p.y/8)
		
		for x=-1,1 do
			for y=-1,1 do
				if mget(tx+x, ty+y) == 85 then
					mset(tx+x, ty+y, 101)
				end
			end
		end
		
	end

	if p.health > p.maxhealth then
		p.health = p.maxhealth
	end
	p.flash-=1
	
	if p.flash < -300 then
		p.health = min(p.maxhealth, p.health+0.05)
	end
	
	p.dx = p.dx * 0.25
	p.dy = p.dy * 0.25
	local speed = p.move_speed_mod
	if btn(0) then
		p.dx = -speed
		p.dir = "left"
	end
	if btn(1) then
		p.dx = speed
		p.dir = "right"
	end
	if btn(2) then
		p.dy = -speed
		p.dir = "up"
	end
	if btn(3) then
		p.dy = speed
		p.dir = "down"
	end
	if btnp(4) then
		p.cycle()
	end
	if btn(5) then
		p.shoot()
	end
	
	move(p)
	
	local moving = abs(p.dx) > 0.1 or abs(p.dy) > 0.1
	
	if moving then
		p.anim -= 1
		
		if p.anim < 0 then
			p.anim += 20
		end
	else
		p.anim = 0
	end
	
	local e = find_nearest(p.x+4, p.y+4, 2, 12)
	
	if e then
