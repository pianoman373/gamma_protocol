function p.update()
	if (p.msg_life>0) p.msg_life-=1
	
	if map_col(p, 1) then
		p.give_gun()
		
		local tx = flr(p.x/8)
		local ty = flr(p.y/8)
		
		for x=-1,1 do
			for y=-1,1 do
				if mget(tx+x, ty+y) == 85 then
					mset(tx+x, ty+y, 101)
				end
			end
		end
		
	end

	if p.health > p.maxhealth then
		p.health = p.maxhealth
	end
	p.flash-=1
	
	if p.flash < -300 then
		p.health = min(p.maxhealth, p.health+0.05)
	end
	
