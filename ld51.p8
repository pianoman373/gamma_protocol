	local s = p.gun().sp
	--set gun onto map
	mset(0, 0, s)
	--add padding
	mset(1, 0, 0)
	mset(0, 1, 0)
	mset(1, 1, 0)
	
	local step = 1/32
 local rot=p.angle\step * step
	
	local gx=p.x+4+cos(rot)*3
	local gy=p.y+4+sin(rot)*3

	--draw gun
