function lvup(x, y)
	for i=1,20 do
		add_particle({
			id=4,
			x = x,
			y = y,
			dx = rnd(4)-2,
			dy = rnd(4)-2,
			fx=0,
			fy=0,
			life = 20
		})
	end
	add_particle({
			id=5,
			x = x,
			y = y,
			dx = 0,
			dy = 0,
			fx=0,
			fy=0,
			life = 20
	})
end

function footstep(x, y)
	add_particle({
			id=1,
			x = x,
			y = y,
			dx = 0,
			dy = -0.1,
			fx=0,
			fy=0,
			life = 10,
			col = 7,
			bgcol = 6,
		})
end

function spawn_ef(x, y, col)
	for i=1,5 do
		add_particle({
			id=2,
			x = x,
			y = y,
			dx = (rnd(2)-1)*0.25,
			dy = -0.8,
			fx=0,
			fy=0.05,
			life = 20,
			col = col
		})
	end
end

function smoke(x, y, col)
	add_particle({
			id=3,
			x = x,
			y = y,
			dx = 1+rnd(1),
			dy = -1-rnd(1),
			fx=0,
			fy=0,
			life = 20,
			col = col,
			r=rnd(3)+2
		})
end

function add_particle(p)
	p.start_life = p.life
	add(particles, p)
end

function update_particles()
	for i in all(particles) do
		i.dx += i.fx
		i.dy += i.fy
	
		i.x += i.dx
		i.y += i.dy
		
		i.life-=1
		
		if (i.life < 0) del(particles, i)
	end
end

function draw_particles()
	for i in all(particles) do
		local t = i.life/i.start_life
	
		if i.id == 0 then
			circfill(i.x, i.y, t*4+1, i.col)
			circfill(i.x, i.y, t*4, 7)
		elseif i.id == 1 then
			circfill(i.x+1, i.y+1, t*2+1, i.bgcol)
			circfill(i.x, i.y, t*2+1, i.col)
		elseif i.id == 2 then
			circfill(i.x, i.y, t*2+1, i.col)
		elseif i.id == 3 then
			circfill(i.x, i.y, t*i.r, i.col)
		elseif i.id == 4 then
			spr(4, i.x, i.y)
		elseif i.id == 5 then
			circ(i.x, i.y, t*32, 7)
		end
	end
end
